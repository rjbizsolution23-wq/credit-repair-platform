{
  "name": "elite-credit-repair-platform",
  "version": "1.0.0",
  "description": "Comprehensive credit repair platform with CRM, dispute management, and AI-powered insights",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:setup": "node scripts/start-dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs": "jsdoc -c jsdoc.conf.json",
    "perf:test": "artillery run tests/performance/load-test.yml",
    "perf:profile": "clinic doctor -- node app.js",
    "perf:flame": "0x app.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset.js",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:fresh": "npm run db:reset -- --force && npm run db:setup",
    "build": "npm run lint && npm test",
    "deploy": "npm run build && npm run start",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "backup:db": "node scripts/backup.js",
    "cleanup:logs": "node scripts/cleanup-logs.js",
    "ai:train": "node scripts/train-ai-model.js"
  },
  "keywords": [
    "credit-repair",
    "crm",
    "dispute-management",
    "ai",
    "machine-learning",
    "billing",
    "document-management",
    "analytics",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": {
    "name": "RJ BUSINESS SOLUTIONS",
    "email": "contact@rjbusinesssolutions.com",
    "url": "https://rjbusinesssolutions.com"
  },
  "license": "AGPL-3.0",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "cron": "^2.4.4",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "docx": "^8.2.2",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-openapi-validator": "^5.0.6",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-slow-down": "^1.6.0",
    "express-validator": "^7.0.1",
    "extract-zip": "^2.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "html-pdf": "^3.0.1",
    "ioredis": "^5.3.2",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mammoth": "^1.6.0",
    "memory-cache": "^0.2.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.5.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "puppeteer": "^21.1.1",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^2.4.2",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.32.5",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stripe": "^13.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tesseract.js": "^4.1.1",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.0",
    "validator": "^13.11.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "0x": "^5.5.0",
    "artillery": "^2.0.0",
    "chai": "^4.3.8",
    "clinic": "^12.1.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "faker": "^6.6.6",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jsdoc": "^4.0.2",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prettier": "^3.0.2",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "wait-on": "^7.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/logs/**",
      "!**/uploads/**",
      "!**/public/**",
      "!**/client/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "contributors": [
    {
      "name": "Rick Jefferson",
      "email": "rick@rjbusinesssolutions.com",
      "url": "https://rickjeffersoncreditsolutions.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rj-business-solutions/rick-jefferson-credit-platform.git"
  },
  "bugs": {
    "url": "https://github.com/rj-business-solutions/rick-jefferson-credit-platform/issues"
  },
  "homepage": "https://rickjeffersoncreditsolutions.com"
}
